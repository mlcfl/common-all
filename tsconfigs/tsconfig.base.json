{
	"compilerOptions": {
		"outDir": "dist",
		"moduleResolution": "node",
		"module": "ESNext",
		"target": "ESNext",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"paths": {
			"~/*": ["src/*"]
		}
	}
}
